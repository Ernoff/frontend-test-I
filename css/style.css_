@font-face {
	font-family: "LaneFont";
	src: url("font/LANENAR.ttf");
}
@font-face {
	font-family: "ArialFont";
	src: url("font/arial.ttf");
}
@font-face {
	font-family: "ArialBoldFont";
	src: url("font/arialbd.ttf");
}
@font-face {
	font-family: "CenturyFont";
	src: url("font/GOTHIC.TTF");
	font-weight: bold;
}
*{
	margin: 0;
	padding: 0;
	box-sizing:border-box;
	font-family: 'LaneFont';
}
nav{
	color: white;
	font-family: LaneFont;
	text-align: right;
	display: block;
	padding: 20px 20px;
}
nav>a{
	color: white;
	display: block;
	text-decoration: none;

}



#logo{
	text-align: center;
	color: white;
	font-size: 100px;
	font-family: Arial;
	letter-spacing: -8px;
	font-weight: bolder;
}
#period{
	font-family: CenturyFont !important;
}

.info{
	font-family: LaneFont;
	color: white;
	text-align: center;
}
.info>a{
	color: white;
	text-decoration: none;
}
#form{

	max-width:330px; 
	margin: 20px auto 20px;
}

div input{
	outline: none;
	display: block;
	width: 100%;
	height: 50px;
	padding-left: 40px;
	background: none ;
	border:none;
	border-bottom: 1px solid white;
	color: white;
}

input::-webkit-input-placeholder
{
	color: #c3c3c3 !important;
	font-size: 14px
}

#submit{
	padding-left: 0px;
	background-color: #0e5296;
	border:none;
	margin-top: 20px;
	border-radius:50px;
	color:white;
	font-family: LaneFont;
	box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);
	transition: all 0.3s ease 0s;
	cursor: pointer;
	outline: none;
}
#submit:hover{
	background-color: #cd333a;
	box-shadow: 0px 15px 20px rgba(38, 131, 208, 0.4);
	transform: translateY(-7px);
}
.icon{
	position: absolute;
	display: inline-block;
	margin-top: 15px;
	height: 20px;

}

#successMessage{
	width: 100%;
	background-color: #339f7b;
	height: 50px;
	color: white;
	text-align: center;
	font-family: CenturyFont;
	border-bottom: 3px solid #26765b;
	line-height: 50px;
}

#errorMessage{
	width: 100%;
	background-color: #d63939;
	height: 50px;
	color: white;
	text-align: center;
	font-family: CenturyFont;
	border-bottom: 3px solid #8d1818;
	line-height: 50px;
}

/*LANDING PAGE*/

#home-head{
	height: calc(100vh - 40px);
	background-image: url('../img/landing.jpg');
	background-size: cover;

}

/*#home-content{*/
	/*height: 30vh;*/
	/*background-color: white;*/
/*}*/

#home-nav{
	text-align: left;
	padding: 30px;
}

#home-nav ul{
	float: right;
	margin-right: 20px;
}

#home-nav li{
	display: inline;
	margin-left: 15px;
}

#home-nav a{
	color: white;
	text-decoration: none;
}
#home-header-content{
	width: 80%;
	margin: 60px auto 0px;
}
#home-title{
	font-size: 55px;
	font-family: ArialBoldFont;
	font-weight: bolder;
	color: white;
	letter-spacing: -3px;
	line-height: 54px;
}
#home-catch{
	font-family: ArialFont;
	color: white;
	font-size: 22px;
	font-weight: 100;
}

#home-get-started{
	background-color: #cd333a;
	padding: 10px;
	border-radius: 50px;
	width: 200px;
	position: relative;
	height: 48px;
	margin-top: 50px;
	box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);
	transition: all 0.3s ease 0s;
	cursor: pointer;
	outline: none;

}

@media (max-width: 374px) {
	#home-get-started{
		margin-top: 18px;
	}
}
#home-get-started:hover{
	box-shadow: 0px 15px 20px rgba(38, 131, 208, 0.4);
	transform: translateY(-7px);
}
#started-icon, #started-text{
	display: inline-block;
	color: white;
	font-family: ArialFont;
	
}
#started-icon{
	position: absolute;
	
}
#started-text{
	margin-left: 40px;
	position: absolute;
	margin-top: 4px;
}

#home-hand{
	position: absolute;
	bottom: 0px;
	right: 5%;
	width:50%;
}

footer{
	width: 100%;
	height: 51px;
	position: absolute;
	bottom: 0px;
	background-color: black;
}
footer>p{
	color: white;
	font-family: CenturyFont;
	text-align: center;
	font-size: 14px;
	padding-top: 10px;
}


