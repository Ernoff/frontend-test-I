/*Aef Styles*/


.uk-parent.uk-open>a {
	color: #cdcdcd !important;
	background-color: #0b2b51;
	line-height: 50px!important;
}
.uk-parent a{
	padding-left: 40px!important;
	text-align: left;
	line-height: 50px!important;
	color: #fff!important;
}
.uk-parent a:after{
	content: '';
	position: relative;
	top: 7px;
}

#logo{
	text-align: center;
	height: 50px;
	margin: 100px 0px;
}
.uk-parent a>img{
	vertical-align: sub;
	height: 30px;
	margin-right: 15px;
	width: 34px;
}

.bg{
	height: 100vh;
	width: 100%;
	background: linear-gradient(rgba(37, 130, 208,0.8),rgba(37, 130, 208,0.8)), url(../img/bg.png);
	background-repeat: no-repeat;
	-webkit-background-size: cover;
	background-size: cover;
}

.white{
	color: #ffffff!important;
}
a.right{
	position: absolute;
	right: 60px;
	display: block;
	text-decoration: none;
}
/*.uk-card-secondary>:not([class*=uk-card-media]) .uk-nav-primary>li>a*/
@media (min-width: 1024px) {
	.headCenter{
		width:60%;
		position: relative;
		left:50%;
		top: 40%;
		-webkit-transform: translate(-50%,-50%);
		-moz-transform: translate(-50%,-50%);
		-ms-transform: translate(-50%,-50%);
		-o-transform: translate(-50%,-50%);
		transform: translate(-50%,-50%);
	}
}

.heading{
	margin-bottom: 15%!important;
}
@media (max-width: 1023px) {
	.headCenter{
		width:80%;
		position: relative;
		left:50%;
		top: 40%;
		-webkit-transform: translate(-50%,-50%);
		-moz-transform: translate(-50%,-50%);
		-ms-transform: translate(-50%,-50%);
		-o-transform: translate(-50%,-50%);
		transform: translate(-50%,-50%);
	}
}
